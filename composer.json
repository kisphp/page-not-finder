{
  "name": "kisphp/page-not-finder",
  "type": "application",
  "description": "A tool to detect 404 pages in your web application",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "guzzle/guzzle": "~3.9",
    "symfony/console": "3.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "fabpot/php-cs-fixer": "~1.9",
    "symfony/var-dumper": "3.0.*"
  },
  "autoload": {
    "psr-4": {
      "Kisphp\\": "src/Kisphp/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Finder\\": "tests/Finder/"
    }
  },
  "bin": ["src/page-not-finder"]
}
